{
    "provides": "archlinux-install-latex",
    "manifest": [],
    "require-sudo": true,
    "require-task": [
        "require-archlinux",
        "archlinux-update",
        "archlinux-install-pacaur"],
    "run-script": [
        ["sh", "-c", "echo $TASKUTIL_SUDO_PASSWORD | sudo -S date"],
        ["pacaur", "-S", "--noedit", "--noconfirm", "--needed",
            "texlive-most"
        ]
    ]
}
