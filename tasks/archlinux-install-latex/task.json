{
    "provides": "archlinux-install-latex",
    "manifest": [],
    "require-sudo": true,
    "require-task": [
        "require-archlinux",
        "archlinux-update"
    ],
    "run-script": [
        ["sh", "-c", "echo $TASKUTIL_SUDO_PASSWORD | sudo -S date"],
        ["pacman", "-S", "--noedit", "--noconfirm", "--needed",
            "texlive-most"
        ]
    ]
}
