{
    "provides": "install-zsh-autosuggestions",
    "manifest": [],
    "require-github": [["zsh-users/zsh-autosuggestions", "zsh-autosuggestions", "master"]],
    "run-script": [
        ["rm", "-rf", "{HOME}/.zsh/zsh-autosuggestions"],
        ["mkdir", "-p", "{HOME}/.zsh/"],
        ["cp", "-r", "zsh-autosuggestions", "{HOME}/.zsh/zsh-autosuggestions"]

    ]
}
