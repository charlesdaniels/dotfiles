{
    "provides": "apply-altera-usbblaster-udev",
    "manifest": ["51.usbblaster.rules"],
    "require-sudo": true,
    "require-task": ["require-linux"],
    "run-script": [
        ["sh", "-c", "echo $TASKUTIL_SUDO_PASSWORD | sudo -S mkdir -p /etc/udev/rles.d"],
        ["sh", "-c", "echo $TASKUTIL_SUDO_PASSWORD | sudo -S cp $TASKUTIL_TASK_DIR/51.usbblaster.rules /etc/udev/rules.d/51.usbblaster.rules"]
    ]
}
