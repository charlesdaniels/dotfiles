if [ "$PROVISION_HAVE_ROOT" = "NO" ] ; then
	# we only want to do a non-privileged install

	# symlink shelldoc in place

	printf "INFO: installing shelldoc..."

	# this assumes ~/bin is where binaries go
	rm -f "$HOME/bin/shelldoc"
	ln -s "$THIRDPARTY_DIR/shelldoc/shelldoc" "$HOME/bin/shelldoc"

	echo " DONE"

fi
